{
    "name": "konecnyjakub/cache",
    "description": "A simple PSR-6 and PSR-16 caching library",
    "license": "BSD-3-Clause",
    "keywords": [
        "cache",
        "caching",
        "simple cache",
        "cache pool",
        "psr-16",
        "psr-6",
        "file",
        "filesystem",
        "memory",
        "apcu"
    ],
    "authors": [
        {
            "name": "Jakub Konečný",
            "email": "jakub.konecny2@centrum.cz",
            "role": "Developer"
        }
    ],
    "support": {
        "sources": "https://gitlab.com/konecnyjakub/cache",
        "issues": "https://gitlab.com/konecnyjakub/cache/-/issues"
    },
    "require": {
        "php": ">=8.3.0",
        "psr/simple-cache": "^2.0 || ^3.0",
        "psr/cache": "^2.0 || ^3.0"
    },
    "require-dev": {
        "php-parallel-lint/php-parallel-lint": "^1.4",
        "php-parallel-lint/php-console-highlighter": "^0.5",
        "squizlabs/php_codesniffer": "^3.11",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "konecnyjakub/mytester": "^7.3"
    },
    "suggest": {
        "ext-apcu": "To use apcu as a cache engine",
        "ext-memcached": "To use memcached as a cache engine",
        "ext-redis": "To use redis as a cache engine",
        "psr/event-dispatcher": "To use events in cache engines"
    },
    "provide": {
        "psr/simple-cache-implementation": "1.0 | 2.0 | 3.0",
        "psr/cache-implementation": "1.0 | 2.0 | 3.0"
    },
    "autoload": {
        "psr-4": {
            "Konecnyjakub\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Konecnyjakub\\Cache\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
